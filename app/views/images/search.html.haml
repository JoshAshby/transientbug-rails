- content_for :page_title do
  Image search

= render "navbar"

- content_for :body do
  %h1 Image Search Results

  %table.table
    %thead
      %tr
        %th Title
        %th Tags
        %th Action

    %tbody
      - @images.each do |image|
        %tr
          %td= image.title
          %td= image.tags.join ", "
          %td= link_to "Show", image
